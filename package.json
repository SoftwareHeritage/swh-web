    "robotstxt-webpack-plugin": "^4.0.1",
    "sass-loader": "^7.0.1",
    "showdown": "^1.8.6",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "typeface-alegreya": "0.0.54",
    "typeface-alegreya-sans": "0.0.54",
    "uglifyjs-webpack-plugin": "^1.2.5",
    "url-loader": "^1.0.1",
    "waypoints": "^4.0.1",
    "webpack": "^4.6.0",
