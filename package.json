    "datatables.net-responsive-bs4": "^2.2.9",
    "dompurify": "^2.3.4",
    "email-validator": "^2.0.4",
    "hex-rgb": "^5.0.0",
    "highlight.js": "^11.4.0",
    "highlightjs-line-numbers.js": "^2.8.0",
    "html-encoder-decoder": "^1.3.9",
    "iframe-resizer": "^4.3.2",
    "intro.js": "^4.3.0",
    "jquery": "^3.6.0",
    "js-cookie": "^3.0.1",
    "js-year-calendar": "^2.0.0",
