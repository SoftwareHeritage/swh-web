swh-web-ui
==========

SWH's web application

# Configuration file sample

~/.config/swh/webapp.ini

    [main]
    # where to log information
    log_dir = /tmp/swh/web-ui/log
    
    # for dev only
    debug = true
    
    # current server (0.0.0.0 for world opening)
    host = 127.0.0.1
    
    # its port
    port = 6543
    
    # the backend this server communicates to
    api_backend = http://127.0.0.1:5000
